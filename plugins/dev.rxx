fn init(session_in, renderer_in) {
    let session = session_in;
    let renderer = renderer_in;
    register_command("hello", "Says hello");
    print(`dev.rxx initialized`);
}

fn mouse_input(state, button, p) {
    print(`mouse_input: ${state} ${button} ${p}`);
}

fn mouse_wheel(delta) {
    print(`mouse_wheel: ${delta}`);
}

fn cursor_moved(p) {
    print(`cursor_moved: ${p}`);
}

fn cmd_hello(args) {
    session.switch_mode(script_mode("hello"));
}